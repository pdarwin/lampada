        </ownedComment>
        <ownedAttribute xmi:id="_ugHjUIv_EeyDmIylgZKhAg" name="now" visibility="package" isStatic="true">
          <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#long"/>
