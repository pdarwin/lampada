        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_8y5KIIxnEeyauKcbfTyhVw" name="errorHandler" specification="_8yF44IxnEeyauKcbfTyhVw">
          <ownedParameter xmi:id="_8y5KIYxnEeyauKcbfTyhVw" name="err" type="_8yEDsYxnEeyauKcbfTyhVw"/>
