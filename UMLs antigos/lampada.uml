          <language>java</language>
          <body>System.out.print(&quot;Está a esfregar a lâmpada &quot; + rubs + &quot; vezes. Quantos desejos pretende realizar?&quot;);&#xD;
		&#xD;
		int numWishes = MyErrorHandler.tryScannerIntFromNextLine(sc);&#xD;
		&#xD;
		if (numGenies > 0)&#xD;
		{&#xD;
			&#xD;
			numGenies -= 1;&#xD;
			if ((rubs % 2 == 0) || (rubs == 1))&#xD;
			{&#xD;
				return new NiceGenie(numWishes);&#xD;
			}&#xD;
			else&#xD;
			{&#xD;
				return new BadGenie();&#xD;
			}&#xD;
			&#xD;
		}&#xD;
		else&#xD;
		{&#xD;
			return new Demon(numWishes);&#xD;
		}&#xD;
	</body>
