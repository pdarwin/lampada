        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mjawwIt2EeyIB7qww0-c5Q" name="main" specification="_mfdd0It2EeyIB7qww0-c5Q">
          <ownedParameter xmi:id="_mjbX0It2EeyIB7qww0-c5Q" name="args">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
